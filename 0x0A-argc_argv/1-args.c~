#include "main.h"
#include <stdio.h>

/**
 * main - prints the number of arguments passed
 * @argc: number of command line arguments
 * @argv: name of array
 * Return: 0
 */

int main(int argc, char *argv[])

{

int i;

if (argc == 1)
{
printf("%d\n", argc - 1);
}

else
{
for (i = 0; *argv; i++; *argv++)
;

}
return (0);


}
